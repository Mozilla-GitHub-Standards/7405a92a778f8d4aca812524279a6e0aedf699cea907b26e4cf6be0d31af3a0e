{
    "template" : "telemetry_agg_*",
    "order" : 0,
    "mappings" : {
        "_default_" : {
            "dynamic_templates": [
            {
                "default_strings": {
                    "path_match": "*",
                    "match_mapping_type": "string",
                    "mapping": {
                        "type": "string",
                        "index": "not_analyzed"
                    }
                }
            }
            ],
            "_all": {
                "enabled": false
            },
            "_source": { 
                "enabled": true,
                "compress": true
            }
        }
    }
}